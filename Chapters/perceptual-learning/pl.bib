@article{Sagi1994,
abstract = {Perceptual learning in vision has been found to be highly specific for simple stimulus attributes, implying highly specific modifications in the nervous system. The type of specificity found (location, orientation, eye) implied plasticity at very early stages of visual processing, where processing modules were believed to be hard-wired and task independent. Recent studies show, however, that learning is task dependent. Studies examining the time course of learning indicate that at least two different learning processes are involved in perceptual learning, reflecting different levels of processing. Perceptual learning appears to be governed by associative rules and to be constrained by system architecture.},
