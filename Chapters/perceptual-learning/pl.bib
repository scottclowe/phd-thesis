keywords = {Acetylcholine,Acetylcholine: pharmacology,Action Potentials,Action Potentials: drug effects,Action Potentials: physiology,Animals,Callithrix,Space Perception,Space Perception: drug effects,Space Perception: physiology,Visual Cortex,Visual Cortex: drug effects,Visual Cortex: physiology},
month = {apr},
number = {4},
