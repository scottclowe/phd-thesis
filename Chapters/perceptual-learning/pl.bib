journal = {The Journal of Physiology},
keywords = {Animals,Cats,Corpus Callosum,Corpus Callosum: physiology,Evoked Potentials,Optic Chiasm,Optic Chiasm: physiology,Space Perception,Space Perception: physiology,Time Factors,Visual},
number = {1},
pages = {619--632},
pmid = {3656156},
title = {{The transfer of visual information across the corpus callosum: spatial and temporal properties in the cat}},
