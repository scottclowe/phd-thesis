keywords = {Attention,Discrimination,Neural coding,Noise correlation,Vision,Visual cortex},
number = {3},
pages = {543--553},
pmid = {18552357},
title = {{Noise correlations have little influence on the coding of selective attention in area V1}},
volume = {19},
year = {2009}
}
