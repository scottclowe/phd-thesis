issn = {1529-2401},
journal = {Journal of Neuroscience},
keywords = {Animals,Attention,Attention: physiology,Contrast Sensitivity,Contrast Sensitivity: physiology,Electrodes,Electrophysiology,Implanted,Macaca mulatta,Male,Neurons,Neurons: physiology,Photic Stimulation,Visual Cortex,Visual Cortex: physiology,Visual Pathways,Visual Pathways: physiology},
month = {sep},
