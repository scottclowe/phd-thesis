issn = {0022-3077},
journal = {Journal of Neurophysiology},
keywords = {Action Potentials,Action Potentials: physiology,Animal,Animal: physiology,Animals,Behavior,Computer Simulation,Conditioning (Psychology),Conditioning (Psychology): physiology,Information Storage and Retrieval,Information Storage and Retrieval: methods,Macaca mulatta,Male,Models,Motor Cortex,Motor Cortex: cytology,Motor Cortex: physiology,Movement,Movement: physiology,Neurological,Neurons,Neurons: physiology,Psychomotor Performance,Psychomotor Performance: physiology,Reaction Time,Reaction Time: physiology,Statistical,Statistics as Topic,Stochastic Processes,Synaptic Transmission,Synaptic Transmission: physiology,Visual Perception,Visual Perception: physiology},
month = {jun},
