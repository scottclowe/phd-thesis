file = {:home/scott/Documents/Mendeley Desktop/2006/Williford, Maunsell - 2006 - Journal of Neurophysiology.pdf:pdf},
issn = {0022-3077},
journal = {Journal of Neurophysiology},
keywords = {Afferent,Afferent: physiology,Animals,Attention,Attention: physiology,Contrast Sensitivity,Contrast Sensitivity: physiology,Evoked Potentials,Macaca mulatta,Macaca mulatta: physiology,Male,Neurons,Orientation,Orientation: physiology,Pattern Recognition,Psychophysiology,Space Perception,Space Perception: physiology,Visual,Visual Cortex,Visual Cortex: physiology,Visual: physiology},
